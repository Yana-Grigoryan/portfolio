    "def Count(fname):\n",
    "    with open(fname) as f:\n",
    "        for i, l in enumerate(f):\n",
    "            pass\n",
    "    return i+1\n",
    "\n",
    "def Sen(fname):\n",
    "    Sen = []\n",
    "    with open(fname) as f:\n",
    "        for line in f:\n",
    "            Sen.append(line.lower())\n",
    "    return Sen"
   ]
  },
