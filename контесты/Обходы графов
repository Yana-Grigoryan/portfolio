    "def weight_category(weight):\n",
    "    pass\n",
    "    if weight < 120:\n",
    "        return 1\n",
    "    elif weight >= 150:\n",
    "        return 3\n",
    "    else:\n",
    "        return 2\n",
    "data['weight_cat'] = data['Weight'].apply(weight_category)\n",
    "sns.boxplot(y=\"Height\", x=\"weight_cat\", data=data)\n",
    "plt.xlabel('Весовая категория');\n",
    "plt.ylabel('Рост')\n",
    "plt.show()"
